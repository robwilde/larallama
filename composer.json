{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The skeleton application for the Laravel framework.",
    "keywords": ["laravel", "framework"],
    "license": "MIT",
    "require": {
        "php": "^8.3",
        "archtechx/enums": "^0.3.2",
        "fakerphp/faker": "^1.23.1",
        "filament/filament": "^3.2.112",
        "flowframe/laravel-trend": "^0.2.0",
        "inertiajs/inertia-laravel": "^1.3",
        "laravel/framework": "^11.23.5",
        "laravel/horizon": "^5.28.1",
        "laravel/jetstream": "^5.2",
        "laravel/pennant": "^1.11",
        "laravel/pulse": "^1.2.5@beta",
        "laravel/reverb": "@beta",
        "laravel/sanctum": "^4.0.2",
        "laravel/socialite": "^5.16",
        "laravel/tinker": "^2.9",
        "league/html-to-markdown": "^5.1.1",
        "maatwebsite/excel": "^3.1.58",
        "malzariey/filament-daterangepicker-filter": "^3.0.4",
        "nwidart/laravel-modules": "^11.1",
        "opcodesio/log-viewer": "^3.11.1",
        "openai-php/laravel": "^0.8.1",
        "owenvoke/blade-fontawesome": "^2.6",
        "pgvector/pgvector": "^0.2.0",
        "phpoffice/phppresentation": "dev-fix-pptx",
        "phpoffice/phpword": "^1.3",
        "pusher/pusher-php-server": "^7.2.4",
        "roach-php/core": "^3.2",
        "roach-php/laravel": "^3.1",
        "saade/filament-fullcalendar": "^3.2.2",
        "smalot/pdfparser": "^2.11",
        "soundasleep/html2text": "^2.1",
        "spatie/browsershot": "^4.3",
        "spatie/laravel-data": "^4.9",
        "spatie/laravel-markdown": "^2.5.2",
        "spatie/laravel-sluggable": "^3.6",
        "sundance-solutions/larachain-token-count": "dev-main",
        "symfony/http-client": "^7.1.4@RC",
        "symfony/postmark-mailer": "^7.1.1@RC",
        "tightenco/ziggy": "^2.3",
        "vedmant/laravel-feed-reader": "^1.6.1",
        "vipnytt/sitemapparser": "^1.3",
        "voku/stop-words": "^2.0.1",
        "wamania/php-stemmer": "^3.0.1",
        "webklex/laravel-imap": "^4.1.2",
        "yethee/tiktoken": "^0.4.0"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.13.5",
        "larastan/larastan": "^2.9.8",
        "laravel/pint": "^1.17.3",
        "laravel/sail": "^1.32",
        "mockery/mockery": "^1.6.12",
        "nunomaduro/collision": "^8.4",
        "phpunit/phpunit": "^10.5.34",
        "spatie/laravel-horizon-watcher": "^1.1",
        "spatie/laravel-ignition": "^2.8"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        },
        "files": [
            "app/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        },
        "files": [
            "app/helpers.php"
        ]
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi",
            "@php artisan filament:upgrade"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi",
            "@php -r \"file_exists('database/database.sqlite') || touch('database/database.sqlite');\"",
            "@php artisan migrate --graceful --ansi"
        ],
        "fix": "vendor/bin/pint",
        "test": "php artisan test",
        "test-local": "XDEBUG_MODE=coverage herd php artisan test --coverage  --min=40",
        "stan": "vendor/bin/phpstan analyse --memory-limit 2G"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/alnutile/php-presentation-fix"
        }
    ],
    "extra": {
        "branch-alias": {
            "dev-master": "11.x-dev"
        },
        "laravel": {
            "dont-discover": [
                "openai-php/laravel"
            ]
        },
        "merge-plugin": {
            "include": [
                "Modules/*/composer.json"
            ]
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true,
            "wikimedia/composer-merge-plugin": true
        }
    },
    "minimum-stability": "beta",
    "prefer-stable": false
}
